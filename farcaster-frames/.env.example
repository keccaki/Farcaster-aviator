# Farcaster Frame Environment Variables

# Application
NEXT_PUBLIC_FRAME_BASE_URL=https://your-aviator-frames.vercel.app
NODE_ENV=production

# Laravel API Integration  
LARAVEL_API_URL=https://your-aviator-backend.com
LARAVEL_API_KEY=your_api_key_here

# Farcaster Configuration
NEYNAR_API_KEY=your_neynar_api_key
FARCASTER_HUB_URL=https://nemes.farcaster.xyz:2283

# Redis (for caching and real-time features)
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=

# Database (if using separate DB for frames)
DATABASE_URL=postgresql://username:password@localhost:5432/aviator_frames

# Analytics & Monitoring
MIXPANEL_PROJECT_TOKEN=your_mixpanel_token
SENTRY_DSN=your_sentry_dsn

# Payment Integration (Farcaster Pay when available)
FARCASTER_PAY_ENABLED=false
FARCASTER_PAY_API_KEY=

# Feature Flags
ENABLE_TOURNAMENTS=true
ENABLE_SOCIAL_PAYMENTS=true
ENABLE_COPY_TRADING=true
ENABLE_VIRAL_SHARES=true

# Security
FRAME_SIGNATURE_SECRET=your_frame_signature_secret
RATE_LIMIT_MAX_REQUESTS=100
RATE_LIMIT_WINDOW_MS=60000

# Image Generation
ENABLE_IMAGE_CACHING=true
IMAGE_CACHE_TTL=300

# Debug
DEBUG_FRAMES=false
LOG_LEVEL=info

